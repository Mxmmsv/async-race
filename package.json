{
  "name": "async-race",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
